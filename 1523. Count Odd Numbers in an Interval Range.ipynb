   "source": [
    "def count_odd(low, high):\n",
    "    diff = high - low\n",
    "\n",
    "    if high % 2 == 0 and low % 2 == 0:\n",
    "        return (diff // 2)\n",
    "    else:\n",
    "        return ((diff//2)+1)\n",
    "\n",
    "\n",
    "\n"
   ]
